{
  "name": "symfony/website-skeleton",
  "type": "project",
  "license": "MIT",
  "description": "A skeleton to start a new Symfony website",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.3",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-iconv": "*",
    "ext-libxml": "*",
    "beberlei/doctrineextensions": "^1.3",
    "doctrine/annotations": "^2.0",
    "doctrine/doctrine-bundle": "^2.5",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^3.2",
    "doskyft/csv-helper": "^1.0",
    "doskyft/helloasso-php": "^1.1",
    "easycorp/easyadmin-bundle": "^4.0",
    "erusev/parsedown": "^1.7",
    "gedmo/doctrine-extensions": "^3.6",
    "liip/imagine-bundle": "^2.8",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpstan/phpdoc-parser": "^1.2",
    "presta/sitemap-bundle": "^4.0",
    "scienta/doctrine-json-functions": "^6.1",
    "sentry/sentry-symfony": "^5.0",
    "symfony/asset": "7.3.*",
    "symfony/asset-mapper": "7.3.*",
    "symfony/console": "7.3.*",
    "symfony/doctrine-messenger": "7.3.*",
    "symfony/dom-crawler": "7.3.*",
    "symfony/dotenv": "7.3.*",
    "symfony/expression-language": "7.3.*",
    "symfony/flex": "^2",
    "symfony/form": "7.3.*",
    "symfony/framework-bundle": "7.3.*",
    "symfony/google-mailer": "7.3.*",
    "symfony/http-client": "7.3.*",
    "symfony/intl": "7.3.*",
    "symfony/mailer": "7.3.*",
    "symfony/messenger": "7.3.*",
    "symfony/mime": "7.3.*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/notifier": "7.3.*",
    "symfony/process": "7.3.*",
    "symfony/property-access": "7.3.*",
    "symfony/property-info": "7.3.*",
    "symfony/runtime": "7.3.*",
    "symfony/security-bundle": "7.3.*",
    "symfony/serializer": "7.3.*",
    "symfony/stimulus-bundle": "^2.12",
    "symfony/string": "7.3.*",
    "symfony/translation": "7.3.*",
    "symfony/twig-bundle": "7.3.*",
    "symfony/uid": "7.3.*",
    "symfony/validator": "7.3.*",
    "symfony/web-link": "7.3.*",
    "symfony/webpack-encore-bundle": "^2.3",
    "symfony/yaml": "7.3.*",
    "twig/cssinliner-extra": "^3.4",
    "twig/extra-bundle": "^3.0",
    "twig/twig": "^3.0",
    "vich/uploader-bundle": "^2.0"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^4.1",
    "fakerphp/faker": "^1.19",
    "friendsofphp/php-cs-fixer": "*",
    "hautelook/alice-bundle": "^2.14",
    "phpstan/phpstan": "^2.1.29",
    "phpstan/phpstan-doctrine": "^2.0.6",
    "phpstan/phpstan-symfony": "^2.0.8",
    "phpunit/phpunit": "^9.6",
    "rector/rector": "^2.1.7",
    "roave/security-advisories": "dev-latest",
    "symfony/browser-kit": "7.3.*",
    "symfony/css-selector": "7.3.*",
    "symfony/debug-bundle": "7.3.*",
    "symfony/doctrine-bridge": "7.3.*",
    "symfony/maker-bundle": "^1.0",
    "symfony/phpunit-bridge": "^7.1",
    "symfony/stopwatch": "7.3.*",
    "symfony/web-profiler-bundle": "7.3.*"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "php-http/discovery": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "importmap:install": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "app:dev-server": "php -S localhost:80 -t public",
    "app:lint": "vendor/bin/phpstan analyse -c tools/phpstan/phpstan.neon",
    "app:cs": "vendor/bin/php-cs-fixer fix src --allow-risky=yes --config=tools/php-cs-fixer/.php-cs-fixer.php",
    "app:tests": [
      "php bin/console --env=test cache:clear",
      "@app:database-init --env=test",
      "php bin/phpunit  --bootstrap tests/bootstrap.php --configuration phpunit.xml.dist",
      "php bin/console --env=test doctrine:database:drop --no-interaction --force"
    ],
    "app:database-init": [
      "php bin/console doctrine:database:drop --no-interaction --if-exists --force",
      "php bin/console doctrine:database:create --no-interaction",
      "php bin/console doctrine:schema:create --no-interaction"
    ],
    "app:fixtures": "php bin/console hautelook:fixtures:load --no-interaction --no-bundles",
    "app:rector": "vendor/bin/rector process src",
    "app:clean": [
      "@app:cs",
      "@app:rector",
      "@app:rector",
      "@app:lint"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.3.*",
      "docker": true
    }
  }
}
